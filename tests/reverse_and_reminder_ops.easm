# prepare data to work with
LI,0x0 0x40         # 0x40 = 64
LI,0x1 0x32         # 0x32 = 50
LI,0x2 0x05
LI,0x3 0x04
LI,0x4 0x01
LI,0xa 0x05         # this will be used for supervisor call, id = 5

# state of registers
# r0 r1 r2 r3 r4 ... ra
# 64 50  5  4  1      5

SVC,0xa 0x00,0
SVC,0xa 0x04,0
SVC,0xa 0x08,0
SVC,0xa 0x0c,0
SVC,0xa 0x28,0

# r4 = r3 - r4 = 4 - 1 = 3
RS,0x4 0xc,0
SVC,0xa 0x10,0
# r4 = 4 - r4 = 4 - 3 = 1
RSI,0x4 4
SVC,0xa 0x10,0
# r4 = r3 / r4 = 4 / 1 = 4
RDR,0x4 0x3
SVC,0xa 0x10,0
