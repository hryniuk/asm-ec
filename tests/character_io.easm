# read character SVC call at register 0
LI,0x0 0x3,0
# write character SVC call at register 1
LI,0x1 0x7,0
# read 4 characters and store them in memory
# 0xff0-0xff3
SVC,0x0 0xff0,0
SVC,0x0 0xff1,0
SVC,0x0 0xff2,0
SVC,0x0 0xff3,0
# write read characters
SVC,0x1 0xff0,0
SVC,0x1 0xff1,0
SVC,0x1 0xff2,0
SVC,0x1 0xff3,0